{
  "name": "osutime",
  "version": "1.0.0",
  "description": "Yeet",
  "main": "server/server.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && nodemon src/server.js",
    "test": "cd server && npm test && cd ../client && npm run myTest",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "install": "npm install && npm run install-all",
    "install-all": "cd server && npm install && cd ../client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michiocre/osuTime.git"
  },
  "author": "Michiocre",
  "license": "Yeet",
  "bugs": {
    "url": "https://github.com/Michiocre/osuTime/issues"
  },
  "homepage": "https://github.com/Michiocre/osuTime#readme",
  "dependencies": {
    "concurrently": "^4.1.0"
  }
}
